{
  "name": "astral_attestation",
  "version": "0.0.1",
  "description": "Kaluga Astral Attestation System",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "env-cmd ./.env ./node_modules/.bin/nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec ' ./node_modules/.bin/ts-node' src/index.ts",
    "start": "ts-node ./src/index.ts",
    "prod": "concurrently \"cd client && npm install && npm run build\" \"npm run build && npm start\"",
    "migration:generate": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:generate -n",
    "migration:run": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run",
    "convert": "./node_modules/.bin/ts-node ./src/scripts/TxtJsonConverter"
  },
  "keywords": [],
  "author": "Igor Osipov <osipov.kaluga@gmail.com> ",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.0",
    "consolidate": "^0.15.1",
    "cookie-parser": "^1.4.3",
    "connect-redis": "^3.3.3",
    "express": "^4.16.3",
    "express-react-views": "^0.10.5",
    "express-session": "^1.15.6",
    "helmet": "^3.12.1",
    "install": "^0.11.0",
    "inversify": "5.0.1",
    "inversify-express-utils": "6.2.0",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "pg": "^7.4.3",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.6"
  },
  "devDependencies": {
    "@types/connect-redis": "0.0.7",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.8",
    "@types/helmet": "0.0.37",
    "@types/inversify": "^2.0.33",
    "@types/node": "^8.10.39",
    "@types/passport": "^0.4.5",
    "@types/passport-google-oauth": "^1.0.38",
    "@types/pg": "^7.4.10",
    "env-cmd": "^8.0.2",
    "nodemon": "^1.18.4",
    "ts-node": "^3.3.0",
    "tslint": "^5.10.0",
    "typescript": "^2.5.2"
  }
}
